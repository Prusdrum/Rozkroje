{
  "name": "rozkroje",
  "version": "0.8.0",
  "engines": {
    "node": "8.6.0",
    "npm": "5.2.0"
  },
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "nodemon ./bin/www",
    "client:build": "set NODE_ENV=production && node ./node_modules/webpack/bin/webpack.js --config config/webpack/webpack.prod.js",
    "client:dev": "set NODE_ENV=development && node ./node_modules/webpack/bin/webpack.js --watch --config config/webpack/webpack.dev.js",
    "test": "node ./node_modules/mocha/bin/mocha server/**/*.test.js",
    "test:dev": "node ./node_modules/mocha/bin/mocha server/**/*.test.js --watch",
    "postinstall": "npm run client:build"
  },
  "nodemonConfig": {
    "ignore": [
      "client/*",
      "node_modules/*"
    ]
  },
  "dependencies": {
    "axios": "0.16.2",
    "babel-core": "6.25.0",
    "babel-loader": "7.1.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.5.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-jest": "21.2.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "body-parser": "~1.17.1",
    "bootstrap": "3.3.7",
    "chai": "4.0.2",
    "cookie-parser": "~1.4.3",
    "css-loader": "0.28.4",
    "debug": "~2.6.3",
    "expose-loader": "0.7.3",
    "express": "~4.15.2",
    "handsontable": "0.32.0",
    "i18n": "0.8.3",
    "jquery": "3.2.1",
    "knockout": "3.4.2",
    "lodash": "4.17.4",
    "mocha": "3.4.2",
    "moment": "2.18.1",
    "morgan": "~1.8.1",
    "node-sass": "^4.9.3",
    "numbro": "1.11.0",
    "pikaday": "1.6.1",
    "promise-polyfill": "6.0.2",
    "pug": "~2.0.0-beta11",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-handsontable": "0.3.1",
    "react-i18nify": "1.8.8",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "redux-actions": "2.2.1",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.1",
    "sass-loader": "^7.1.0",
    "serve-favicon": "~2.4.2",
    "style-loader": "0.18.2",
    "webpack": "3.0.0",
    "winston": "2.3.1",
    "zeroclipboard": "2.3.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.4"
  }
}
